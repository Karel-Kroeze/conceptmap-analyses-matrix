{
    "name": "concept-map-analyzer-matrix",
    "version": "2.0.0",
    "source": "src/index.ts",
    "main": "lib/index.js",
    "types": "dist/analyzer.d.ts",
    "global": "dist/analyzer.js",
    "scripts": {
        "test": "jest",
        "watch": "concurrently -n tsc,build -c blue,green \"yarn dev:watch\" \"yarn graasp:watch\"",
        "dev:watch": "tsc -w",
        "graasp:build": "start build.bat",
        "graasp:watch": "sane \"npm run graasp:build\" --wait=10 ./src"
    },
    "targets": {
        "global": {
            "isLibrary": false,
            "context": "browser",
            "includeNodeModules": true,
            "outputFormat": "global",
            "scopeHoist": true
        }
    },
    "engines": {
        "browsers": "> 0.2%"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/cytoscape": "^3.1.10",
        "@types/jest": "^25.2.3",
        "@types/lodash": "^4.14.149",
        "@types/mathjs": "^6.0.4",
        "@types/node": "^13.9.3",
        "@types/vis": "^4.21.19",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "eslint": "^6.8.0",
        "jest": "^25.3.0",
        "parcel": "^2.0.0-alpha.3.2",
        "prettier": "^2.0.2",
        "sane": "^4.1.0",
        "ts-jest": "^25.2.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "levenshtein-edit-distance": "^2.0.5",
        "lodash": "^4.17.19",
        "mathjs": "^6.6.1",
        "vis": "^4.21.0"
    }
}
